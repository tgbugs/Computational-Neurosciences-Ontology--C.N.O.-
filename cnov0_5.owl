@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix definition: <http://purl.obolibrary.org/obo/IAO_0000115> .
@prefix defSource: <http://purl.obolibrary.org/obo/IAO_0000119> .
@prefix hasPart: <http://purl.obolibrary.org/obo/BFO_0000051> .
@prefix IAO: <http://purl.obolibrary.org/obo/IAO_> .
@prefix OBI: <http://purl.obolibrary.org/obo/OBI_> .
@prefix OBOANN: <http://ontology.neuinfo.org/NIF/Backend/OBO_annotation_properties.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix SBO: <http://purl.obolibrary.org/obo/SBO_> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl> a owl:Ontology ;
    owl:imports <http://ontology.neuinfo.org/NIF/ttl/OBO_annotation_properties.ttl>,
        <http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl>,
        <http://purl.obolibrary.org/obo/ro.owl>,
        <http://purl.org/incf/ontology/Computational_Neurosciences/external_import/external.owl>,
        <http://www.w3.org/2004/02/skos/core> ;
    dc:title "Computational Neuroscience Ontology"^^xsd:string ;
    rdfs:comment """This ontology is used to add the Definition class of annotation as in NIF.
The source of the definition can be defined with class from OBO-annotation"""^^xsd:string,
        "This work is licensed under the Creative Commons Attribution-NonCommercial-ShareAlike 3.0 Unported License. To view a copy of this license, visit http://creativecommons.org/licenses/by-nc-sa/3.0/ or send a letter to Creative Commons, 444 Castro Street, Suite 900, Mountain View, California, 94041, USA."^^xsd:string ;
    owl:versionInfo "version 0.5"^^xsd:string ;
    dc:contributor "Birgit Kriener"^^xsd:string,
        "Erik De Schutter"^^xsd:string,
        "Fahim T. Imam"^^xsd:string,
        "INCF MultiScale Modeling Task Force"^^xsd:string,
        "Lars Schwabe"^^xsd:string,
        "Padraig Gleeson"^^xsd:string,
        "Sean Hill"^^xsd:string,
        "Stephen D. Larson"^^xsd:string,
        "Subhasis Ray"^^xsd:string ;
    dc:creator "Yann Le Franc"^^xsd:string ;
    dc:date "13/03/2012"^^xsd:string ;
    dc:subject "An ontology to describe the field of Computational Neurosciences"^^xsd:string .

### Annotation Properties

definition: a owl:AnnotationProperty .

defSource: a owl:AnnotationProperty .

### Data Properties

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000193> a owl:DatatypeProperty ;
    rdfs:label "name"^^xsd:string ;
    rdfs:domain BFO:0000031 ;
    rdfs:range xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000204> a owl:DatatypeProperty ;
    rdfs:label "numerical value"^^xsd:string ;
    rdfs:domain BFO:0000031 ;
    rdfs:range xsd:string .

### Classes

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000001> a owl:Class ;
    rdfs:label "defined model type"^^xsd:string ;
    skos:altLabel "defined type of model"^^xsd:string ;
    skos:definition "a generically_dependent_continuant that is a category of models regarded as sharing particular characteristics, qualities, and/or defined similarities."^^xsd:string ;
    rdfs:subClassOf BFO:0000031 ;
    owl:disjointWith skos:Collection,
        skos:Concept,
        skos:ConceptScheme ;
    rdfs:comment "definition based on the following NCI thesaurus entry: http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#Type_of_Event"^^xsd:string,
        "This general class includes the most common types of models classified based on the level of description of the nervous system."^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000002> a owl:Class ;
    rdfs:label "model component"^^xsd:string ;
    skos:altLabel "model element"^^xsd:string ;
    skos:definition "a generically dependent continuant that is a constituent part of a model"^^xsd:string ;
    rdfs:subClassOf BFO:0000031 ;
    owl:disjointWith skos:Collection,
        skos:Concept,
        skos:ConceptScheme ;
    rdfs:comment "definition based on element from NCI Thesaurus: http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#Element_Part"^^xsd:string,
        "This class is based on the assumption that models can represented as particular assemblies of components that can be either specific to the different level of description (cellular, synaptic and network) or generic."^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000003> a owl:Class ;
    rdfs:label "parameter"^^xsd:string ;
    skos:definition "elementary model component role that defines an elementary model component as constant value determining the characteristics or behaviour of the model"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000147> ;
    owl:disjointWith skos:Collection,
        skos:Concept,
        skos:ConceptScheme ;
    rdfs:comment ""^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000004> a owl:Class ;
    rdfs:label "reset potential"^^xsd:string ;
    skos:definition "value of the membrane potential at which the model is maintained after the emission of a spike."^^xsd:string ;
    rdfs:subClassOf SBO:0000259 ;
    owl:disjointWith skos:Collection,
        skos:Concept,
        skos:ConceptScheme ;
    rdfs:comment ""^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000005> a owl:Class ;
    rdfs:label "physical constant"^^xsd:string ;
    skos:definition "A physical constant that is required in the calculation of a system parameter" ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000181> ;
    OBOANN:definingCitationID "SBO:0000565" ;
    OBOANN:definingCitationURI "http://en.wikipedia.org/wiki/Physical_constant" .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000007> a owl:Class ;
    rdfs:label "phenomenological plasticity model"^^xsd:string ;
    skos:definition "long term plasticity model that is based on an input-output relationship between neuronal activity and synaptic plasticity. This type of model does not explicitly account for the biochemistry and physiology leading to synaptic plasticity."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000183> ;
    defSource: "http://www.scholarpedia.org/article/Models_of_synaptic_plasticity"^^xsd:string ;
    OBOANN:definingCitationURI "http://www.scholarpedia.org/article/Models_of_synaptic_plasticity"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000008> a owl:Class ;
    rdfs:label "cellular model"^^xsd:string ;
    skos:definition "a defined type of model that represent the biological activities of a cell."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000001> ;
    rdfs:comment "The distinction between the different subclasses is based on the different \"spiking mechanisms\"."^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000009> a owl:Class ;
    rdfs:label "synapse model"^^xsd:string ;
    skos:definition "a defined type of model that represents the biological processes involved in synaptic transmission"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000001> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000010> a owl:Class ;
    rdfs:label "network model"^^xsd:string ;
    skos:definition "a defined type of model that represents biological neural networks."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000001> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000011> a owl:Class ;
    rdfs:label "plasticity model"^^xsd:string ;
    skos:definition "a defined type of model that describes the mechanisms underlying the plasticity of the nervous system."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000001> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000012> a owl:Class ;
    rdfs:label "artificial neuron model"^^xsd:string ;
    skos:definition "a cellular model that represents the electrical activity of a nerve cell with simplified function"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000008> ;
    rdfs:comment "This class can be further subdivided into binary neuron, McCullogh neurons and all variants of the artificial neuron models."^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000013> a owl:Class ;
    rdfs:label "point process model"^^xsd:string ;
    skos:definition "a cellular model that reproduce particular features of real neuron spiking patterns with statistical point process models."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000008> ;
    rdfs:comment ""^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000014> a owl:Class ;
    rdfs:label "spiking model"^^xsd:string ;
    skos:definition "a cellular model that describes the mechanism of action potential emission in biological nerve cell."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000008> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000015> a owl:Class ;
    rdfs:label "balanced random network"^^xsd:string ;
    skos:definition "defined model that corresponds to a network of sparsely connected excitatory and inhibitory neurons which can switch between firing states depending on the balance between excitation and inhibition."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> ;
    OBOANN:definingCitationID "PMID: 10809012"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000016> a owl:Class ;
    rdfs:label "Hopfield network"^^xsd:string ;
    skos:definition "defined model that is a form of recurrent artificial neural network invented by John Hopfield."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> ;
    OBOANN:definingCitationURI "http://en.wikipedia.org/wiki/Hopfield_network"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000017> a owl:Class ;
    rdfs:label "biophysical spiking model"^^xsd:string ;
    skos:definition "a spiking model that describe the interactions between the ionic conductances that participates to the generation of an action potential"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000014> ;
    owl:disjointWith <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000018> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000018> a owl:Class ;
    rdfs:label "threshold-based spiking model"^^xsd:string ;
    skos:definition "a spiking model that represents the spiking mechanism as the result of a threshold crossing."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000014> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000019> a owl:Class ;
    rdfs:label "reduced model"^^xsd:string ;
    skos:definition "biophysical model that is obtained by dimensionality reduction of a detailed model."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000017> ;
    rdfs:comment """This class could become a superclass called dynamical models or dynamical system models in contrast with biophysical and threshold based models.
It should contain models like Fitzhugh-Nagumo, Morris-Lecar models."""^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000020> a owl:Class ;
    rdfs:label "detailed model"^^xsd:string ;
    skos:definition "biophysical model that describes in detail the conductance variations of the ionic currents responsible of the generation of an action potential"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000017> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000021> a owl:Class ;
    rdfs:label "pulse-based model"^^xsd:string ;
    skos:definition "a threshold based spiking neuron model that is an hybrid between Hodgkin and Huxley and integrate-and-fire models. The model describes the impact of  fast sodium and delayed rectifier potassium conductances on the subthreshold membrane voltage dynamic and considers action potentials as pulses during which the sodium and potassium conductances are set to particular values representing their participation to the generation and repolarization of the action potential. "^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000018> ;
    rdfs:comment "This classe refers to detailed integrate-and-fire models as described in Destexhe A. (1997, PMID: 9097470) and Hill S. and Tononi G. (2005, PMID: 15537811) "^^xsd:string ;
    altTerm: "conductance-based model"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000022> a owl:Class ;
    rdfs:label "one variable model"^^xsd:string ;
    skos:definition "a threshold-based model that uses only one variable to describe the nerve cells behavior."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000018> ;
    rdfs:comment ""^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000023> a owl:Class ;
    rdfs:label "two variable model"^^xsd:string ;
    skos:definition "a threshold-based model that uses two variables to describe the nerve cells behavior."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000018> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000024> a owl:Class ;
    rdfs:label "three variable model"^^xsd:string ;
    skos:definition "a threshold-based model that uses three variables to describe the nerve cells behavior"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000018> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000025> a owl:Class ;
    rdfs:label "spike response model"^^xsd:string ;
    skos:definition "a threshold-based model that correspond to a generalization of the leaky integrate-and-fire model and give a simple description of action potential generation in neurons"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000018> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000026> a owl:Class ;
    rdfs:label "current-based model"^^xsd:string ;
    skos:definition "a model of chemical synapse that consider the impact of the chemical release as a current source in the post-synaptic model with the postsynaptic membrane voltage considered to be constant."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000031> ;
    owl:disjointWith <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000027> ;
    rdfs:comment ""^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000027> a owl:Class ;
    rdfs:label "conductance-based model"^^xsd:string ;
    skos:definition "a model of chemical synapse that consider the impact of the chemical release as a modification of conductance in the post-synaptic model with the post-synaptic potential varying over time."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000031> ;
    rdfs:comment ""^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000028> a owl:Class ;
    rdfs:label "rate model"^^xsd:string ;
    skos:definition "network model composed of rate-based neuron models"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000010> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000029> a owl:Class ;
    rdfs:label "artificial neural network model"^^xsd:string ;
    skos:definition "network model composed of artificial neuron models."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000010> ;
    rdfs:comment "corresponds to Kohonen maps, Multiple Layer perceptron, ..."^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000030> a owl:Class ;
    rdfs:label "spiking network model"^^xsd:string ;
    skos:definition "network model that is composed of spiking neuron models."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000010> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000031> a owl:Class ;
    rdfs:label "chemical synapse model"^^xsd:string ;
    skos:definition "a synapse model that describes the impact of chemical compounds released by the synapse"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000009> ;
    owl:disjointWith <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000032> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000032> a owl:Class ;
    rdfs:label "electrical synapse model"^^xsd:string ;
    skos:definition "a synapse model that describe synapses transmitting directly ionic currents."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000009> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000033> a owl:Class ;
    rdfs:label "cellular plasticity model"^^xsd:string ;
    skos:definition "plasticity model that describes the variations of the cellular intrinsic excitability due to regulation of ionic conductances depending on the network activity."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000011> ;
    rdfs:comment "This class would contains models that describes the link between potassium channels and NMDA activation as described in hippocampus for instance."^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000034> a owl:Class ;
    rdfs:label "synaptic plasticity model"^^xsd:string ;
    skos:altLabel "synaptic plasticity rule"^^xsd:string ;
    skos:definition "model of plasticity that represents the mechanisms underlying changes in synaptic transmission, depending on the network or cellular activity."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000011> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000035> a owl:Class ;
    rdfs:label "short term facilitation model"^^xsd:string ;
    skos:definition "a model of short term synaptic plasticity that represents the increase of the synaptic efficacy for a short period of time."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000182> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000036> a owl:Class ;
    rdfs:label "cellular model component"^^xsd:string ;
    skos:definition "aggregated model composed that is used to build cellular models"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000218> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000037> a owl:Class ;
    rdfs:label "synapse model component"^^xsd:string ;
    skos:definition "aggregated model component that is used to build synaptic models."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000218> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000038> a owl:Class ;
    rdfs:label "resting membrane potential"^^xsd:string ;
    skos:definition "value of the neuronal membrane potential at rest"^^xsd:string ;
    rdfs:subClassOf SBO:0000259 ;
    rdfs:comment ""^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000039> a owl:Class ;
    rdfs:label "refractory mechanism"^^xsd:string ;
    skos:definition "a cellular model component that represents the mechanism by which the model is kept unresponsive to any stimulus after the emission of an action potential."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000036> ;
    rdfs:comment "This is a general term that encompass the refractory period usually defined with a time constant and the resetting of the membrane potential during the refractory period."^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000040> a owl:Class ;
    rdfs:label "model quality"^^xsd:string ;
    skos:definition "quality that represents a qualitative feature of the model that can be either independent of the model specification or a property resulting of the particular model specification"^^xsd:string ;
    rdfs:subClassOf BFO:0000019 ;
    owl:disjointWith skos:Collection,
        skos:Concept,
        skos:ConceptScheme .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000041> a owl:Class ;
    rdfs:label "spiking mechanism"^^xsd:string ;
    skos:definition "a cellular model component that represents the mechanisms generating action potential in a cellular model type."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000036> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000042> a owl:Class ;
    rdfs:label "morphology"^^xsd:string ;
    skos:definition "a cellular model component that represents the morphology of the model"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000036> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000043> a owl:Class ;
    rdfs:label "intracellular dynamic"^^xsd:string ;
    skos:definition "a cellular model component that represents a chemical signaling occuring inside the nerve cell"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000036> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000044> a owl:Class ;
    rdfs:label "stimulation current"^^xsd:string ;
    skos:definition "current model that represents input currents provided to the model via an electrode (intracellular or extracellular)."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000126> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000045> a owl:Class ;
    rdfs:label "generic model component"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000218> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000046> a owl:Class ;
    rdfs:label "probabilistic component"^^xsd:string ;
    skos:definition "a generic model component that represents the probabilistic variations of any model component."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000045> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000047> a owl:Class ;
    rdfs:label "ionic current model"^^xsd:string ;
    skos:definition "a current model that describes the flow of ions through cellular membrane."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000126> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000048> a owl:Class ;
    rdfs:label "synaptic current"^^xsd:string ;
    skos:definition "current model that represents the flow of ions triggered by the activation of a chemical or electrical synapse model."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000126> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000049> a owl:Class ;
    rdfs:label "abstract current model"^^xsd:string ;
    skos:definition "a current model that is an abtraction of complex current interactions and which is used to reproduce a particular cellular behavior such as rebound or adaptation."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000126> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000050> a owl:Class ;
    rdfs:label "continuous"^^xsd:string ;
    skos:definition "a spiking mechanism that considers the action potential initiation as a continuous process often based on the non linearity of the conductance changes supporting the initiation of an action potential. This mechanism is the opposite of a threshold-based mechanisms."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000041> ;
    rdfs:comment ""^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000051> a owl:Class ;
    rdfs:label "threshold-based"^^xsd:string ;
    skos:definition "a spiking mechanism that considers the action potential initiation as a result of a thresholding operation."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000041> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000052> a owl:Class ;
    rdfs:label "point"^^xsd:string ;
    skos:definition "a morphology that is used to represents the models without specified geometry."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000042> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000053> a owl:Class ;
    rdfs:label "single compartment"^^xsd:string ;
    skos:definition "a morphology that consider the cellular morphology as a unique compartment"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000042> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000054> a owl:Class ;
    rdfs:label "multiple compartment"^^xsd:string ;
    skos:definition "a morphology that represents the modeled cell with multiple compartments. This morphology spans the models called the Ball-and-stick model and the models based on reconstructed morphologies."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000042> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000055> a owl:Class ;
    rdfs:label "reconstructed"^^xsd:string ;
    skos:definition "morphological quality that represents model morphologies build upon the reconstruction of a real nerve cell morphology using imaging techniques such as camera lucida or two-photon imaging."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000165> ;
    rdfs:comment ""^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000056> a owl:Class ;
    rdfs:label "calcium dynamic"^^xsd:string ;
    skos:definition "an intracellular dynamic model component that represents the variation of the intracellular calcium concentration over time."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000043> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000057> a owl:Class ;
    rdfs:label "biochemical dynamic"^^xsd:string ;
    skos:definition "an intracellular dynamic model component that represents the dynamic of the intracellular biochemical reactions and molecular participant concentrations over time."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000043> ;
    rdfs:comment "NOTE: Shouldn't be separated from calcium dynamic as calcium is also a molecular participant. One possible solution would be to change into: reduced calcium dynamic to represent the simplification of buffering and detailed biochemical reactions that details the calcium buffering!!!"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000058> a owl:Class ;
    rdfs:label "network model component"^^xsd:string ;
    skos:definition "a model component that is used to build network model types."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000218> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000059> a owl:Class ;
    rdfs:label "non linear spike generation current"^^xsd:string ;
    skos:definition "abstract current model that represents an additional non-linear current ψ which leads to a divergence of the potential toward infinity in a finite time during the emission of an action potential."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000049> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty hasPart: ;
            owl:someValuesFrom <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000002> ] ;
    rdfs:label "defined model"^^xsd:string ;
    skos:definition "a generically_dependent_continuant that represent the set of  models commonly used to build other models."^^xsd:string ;
    rdfs:subClassOf BFO:0000031 ;
    owl:disjointWith skos:Collection,
        skos:Concept,
        skos:ConceptScheme .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000061> a owl:Class ;
    rdfs:label "integrate-and-fire"^^xsd:string ;
    skos:definition "a defined model that represent the evolution of voltage as the derivative of the law of capacitance. The action potential is emitted when the value of the voltage reaches a threshold and is considered as Dirac function. After the emission the voltage is resetted to the resting potential value."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> ;
    OBOANN:definingCitationURI "http://en.wikipedia.org/wiki/Biological_neuron_model#Integrate-and-fire"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000062> a owl:Class ;
    rdfs:label "adaptive exponential integrate-and-fire"^^xsd:string ;
    OBOANN:synonym "AdEx"^^xsd:string ;
    skos:definition "a defined model that is a spiking neuron model with two variables. The first equation describes the dynamics of the membrane potential and includes an activation ter with an exponential voltage dependence. Voltage is coupled to a second equation which describes adaptation. Both variables are reset if an action potential has been triggered. This model has been introduced by Brette and Gerstner in 2005."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> ;
    OBOANN:definingCitationURI "http://www.scholarpedia.org/article/Adaptive_exponential_integrate-and-fire_model"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000063> a owl:Class ;
    rdfs:label "independent variable"^^xsd:string ;
    skos:definition "variable regarded as inputs to a system and may take on different values freely"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000219> ;
    OBOANN:definingCitationURI "http://en.wikipedia.org/wiki/Variable_(mathematics)"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000064> a owl:Class ;
    rdfs:label "dependent variable"^^xsd:string ;
    skos:definition "variable regarded as values that change as a consequence of changes in other values in the system."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000219> ;
    OBOANN:definingCitationURI "http://en.wikipedia.org/wiki/Variable_(mathematics)"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000065> a owl:Class ;
    rdfs:label "exponential integrate-and-fire"^^xsd:string ;
    OBOANN:synonym "EIF"^^xsd:string ;
    skos:definition "a defined model that is an integrate-and-fire model in which the action potentials are generated by an exponential current instead of a fixed threshold. The exponential integrate-and-fire model is defined in Fourcaud-Trocme and al. 2003"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> ;
    OBOANN:definingCitationURI "w.ploscompbiol.org/article/info:doi/10.1371/journal.pcbi.1001056"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000066> a owl:Class ;
    rdfs:label "leaky integrate-and-fire"^^xsd:string ;
    OBOANN:synonym "LIF, Lapicque's model"^^xsd:string ;
    skos:definition "a defined model that is an integrate-and-fire model with a leak component added to the membrane potential, reflecting the diffusion of ions that occurs through the membrane when some equilibrium is not reached in the cell."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> ;
    OBOANN:definingCitationURI "http://en.wikipedia.org/wiki/Biological_neuron_model#Leaky_integrate-and-fire"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000067> a owl:Class ;
    rdfs:label "quadratic integrate-and-fire"^^xsd:string ;
    OBOANN:synonym "QIF"^^xsd:string ;
    skos:definition "a defined model that is an integrate-and-fire model in which the action potentials are generated by a quadratic current instead of a fixed threshold. The quadratic integrate-and-fire model is defined in Latham and al. 2000. This model is identical to the Theta neuron proposed by Ermentrout and Kopell (1986) with a variable change."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000068> a owl:Class ;
    rdfs:label "theta neuron model"^^xsd:string ;
    skos:definition "a defined model that is well suited to describe the processes involving rapid changes in membrane potential, known as bursts. The theta model describes parabolic bursting and has been defined in Ermentrout and Kopell, SIAM journal of Applied Mathematics 1986."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> ;
    OBOANN:definingCitationURI "http://en.wikipedia.org/wiki/Theta_model"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000069> a owl:Class ;
    rdfs:label "adaptive integrate-and-fire"^^xsd:string ;
    OBOANN:synonym "ALIF"^^xsd:string ;
    skos:definition "a defined model that is a two-variable integrate-and-fire model. The second variable accounts for the currents involved in spike frequency adaptation. This variable increase transiently during action potentials and decay between action potential."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000070> a owl:Class ;
    rdfs:label "generalized integrate-and-fire"^^xsd:string ;
    OBOANN:synonym "GIF"^^xsd:string ;
    skos:definition "a defined model that is a two variable integrate-and-fire model with a threshold for spike generation and a reset as in the leaky integrate-and-fire model. The two variables represent subthreshold resonance behavior and result from the linearization of the Hodgkin and Huxley equation around a subthreshold holding potential. This model is similar to the resonate-and-fire model proposed by Izhikevich (2001)"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> ;
    OBOANN:definingCitationID "PMID: 12611957"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000071> a owl:Class ;
    rdfs:label "integrate-and-fire-or-burst"^^xsd:string ;
    OBOANN:synonym "IFB"^^xsd:string ;
    skos:definition "a defined model that is a two-variable integrate-and-fire model. The second variable is a slow variable that represents the inactivation of the low-threshold calcium current. This model reproduces the features of the bursting behavior of thalamo-cortical relay neurons and has been proposed by Smith and al., 2000"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000072> a owl:Class ;
    rdfs:label "Izhikevich model"^^xsd:string ;
    skos:definition "a defined model that is a minimal two-variable integrate-and-fire model that reproduces different features depending on the value of 4 parameters. The second variable represents the adaptation. This model has been proposed in Izhikevich, 2003."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> ;
    OBOANN:definingCitationID "PMID: 18244602"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000073> a owl:Class ;
    rdfs:label "resonate-and-fire"^^xsd:string ;
    skos:definition "a defined model that is a two variable integrate-and-fire model proposed by Izhikevich in Izhikevich, 2001."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> ;
    OBOANN:definingCitationID "PMID: 11665779"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000074> a owl:Class ;
    rdfs:label "HindMarch and Rose model"^^xsd:string ;
    skos:definition "a defined model that is a three-variable model: the membrane potential written in dimensionless unit, the spiking variable that represent the fast transport of sodium and potassium ions and the bursting variable that represent the slow transport of other ions. This model aims to study the spiking-bursting behavior of the membrane potential observed in experiments."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> ;
    OBOANN:definingCitationID "PMID: 6144106"^^xsd:string ;
    OBOANN:definingCitationURI "http://en.wikipedia.org/wiki/Hindmarsh%E2%80%93Rose_model"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000075> a owl:Class ;
    rdfs:label "Rotstein model"^^xsd:string ;
    skos:definition "a defined model that is three variable reduced model defined in Rotstein and al. 2006 in J. Comp. Neurosci."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> ;
    OBOANN:definingCitationID "PMID: 16927211"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000076> a owl:Class ;
    rdfs:label "Shinomoto model"^^xsd:string ;
    skos:definition "a defined model that is a multi-timescale adaptive threshold model of neuronal spiking incorporated into the stochastic framework of the linear-nonlinear Poisson model in the form of a generalized linear model."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> ;
    OBOANN:definingCitationID "PMID: 20478693"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000077> a owl:Class ;
    rdfs:label "spike response model"^^xsd:string ;
    skos:definition "a defined model that is a generalization of the leaky integrate-and-fire model. As in the leaky integrate-and-fire model, action potentials are generated when the voltage passes a threshold. Leaky integrate-and-fire models are formulated using differential equations for the voltage, whereas the Spike Response Model is formulated using filters."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> ;
    OBOANN:definingCitationID "doi:10.1162/neco.1997.9.5.1015"^^xsd:string ;
    OBOANN:definingCitationURI "http://icwww.epfl.ch/~gerstner/SPNM/node27.html"^^xsd:string,
        "http://www.scholarpedia.org/article/Spike-response_model"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000078> a owl:Class ;
    rdfs:label "spike response model SRM0"^^xsd:string ;
    skos:definition "a defined model that is a simplified version of the Spike Response Model and does not include a dependence of the response kernel κ upon the time since the last spike."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> ;
    OBOANN:definingCitationID "PMID: 16633938"^^xsd:string ;
    OBOANN:definingCitationURI "http://icwww.epfl.ch/~gerstner/SPNM/node27.html#SECTION02323000000000000000"^^xsd:string,
        "http://www.scholarpedia.org/article/Spike-response_model#Spike_Response_Model_SRM.5C.28_0.5C.29"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000079> a owl:Class ;
    rdfs:label "cumulative spike response model"^^xsd:string ;
    skos:definition "a defined model that is a spike response model where refractoriness and adaptation were modeled by the combined effects of the spike after potentials of several previous spikes, rather than only the most recent spike."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> ;
    OBOANN:definingCitationID "PMID: 8888612"^^xsd:string ;
    OBOANN:definingCitationURI "http://www.scholarpedia.org/article/Spike-response_model#Cumulative_Spike_Response_Model:_bursting_and_adaptation"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000080> a owl:Class ;
    rdfs:label "spike response model with cumulative dynamic threshold"^^xsd:string ;
    skos:definition "a defined model that is a spike response model in which the value of the threshold depends on all previous spikes not only the most recent one."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> ;
    OBOANN:definingCitationURI "http://www.scholarpedia.org/article/Spike-response_model#Spike_Response_Model_with_a_cumulative_dynamic_threshold"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000081> a owl:Class ;
    rdfs:label "dynamic threshold"^^xsd:string ;
    skos:definition "a threshold-based spiking mechanism that represent the variation of the spiking threshold over time based on particular criterions."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000051> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000082> a owl:Class ;
    rdfs:label "fixed threshold"^^xsd:string ;
    skos:definition "a threshold-based spiking mechanism that considers the threshold as constant over time."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000051> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000083> a owl:Class ;
    rdfs:label "spike shape kernel"^^xsd:string ;
    skos:definition "a cellular model component that is used to build spike response model and which describes the form of the action potential and its spike after-potential. This kernel is a function of the time since the last action potential. It can describe a depolarizing, hyperpolarizing or resonating spike after-potential."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000036> ;
    OBOANN:definingCitationURI "http://www.scholarpedia.org/article/Spike-response_model#Mathematical_formulation"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000084> a owl:Class ;
    rdfs:label "responsiveness kernel"^^xsd:string ;
    OBOANN:synonym "linear filter of the membrane"^^xsd:string ;
    skos:definition "a cellular model component that is used to build spike response model and which describes the linear response to an incoming short pulse. The responsiveness kernel to an input pulse depends on the time since the last spike."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000036> ;
    OBOANN:definingCitationURI "http://www.scholarpedia.org/article/Spike-response_model#Mathematical_formulation"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000085> a owl:Class ;
    rdfs:label "adaptation current"^^xsd:string ;
    skos:definition "an abstract current model that represents the mechanism of spiking frequency adaptation."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000049> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000086> a owl:Class ;
    rdfs:label "rebound current"^^xsd:string ;
    skos:definition "abstract current model that represents the mechanisms of rebound."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000049> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000087> a owl:Class ;
    rdfs:label "spiking current"^^xsd:string ;
    OBOANN:synonym "spiking variable, y"^^xsd:string ;
    skos:definition "abstract current model that represents the mechanisms involved in the action potential and which is used to build HindMarch and Rose models"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000049> ;
    rdfs:comment ""^^xsd:string ;
    OBOANN:definingCitationURI "http://en.wikipedia.org/wiki/Hindmarsh%E2%80%93Rose_model"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000088> a owl:Class ;
    rdfs:label "bursting current"^^xsd:string ;
    OBOANN:synonym "bursting variable, z"^^xsd:string ;
    skos:definition "abstract current model that represents the mechanisms of bursting and used to build HindMarch and Rose models."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000049> ;
    rdfs:comment ""^^xsd:string ;
    OBOANN:definingCitationURI "http://en.wikipedia.org/wiki/Hindmarsh%E2%80%93Rose_model"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000089> a owl:Class ;
    rdfs:label "membrane recovery variable"^^xsd:string ;
    skos:definition "abstract current model which accounts for the activation of potassium ionic currents and inactivation of sodium ionic currents and provides negative feedback to the membrane voltage"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000049> ;
    rdfs:comment ""^^xsd:string ;
    OBOANN:definingCitationID "PMID: 18244602"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000090> a owl:Class ;
    rdfs:label "Connor-Stevens model"^^xsd:string ;
    skos:definition "a defined model that is an extended action potential generating model using gastropod somas. This model is similar to the Hodgkin and Huxley model and contains a sodium, potassium and leak current with faster dynamics for the sodium and potassium currents. In addition, a transient potassium current, called, A-current, has been included."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> ;
    OBOANN:definingCitationID "PMID: 5575343"^^xsd:string ;
    OBOANN:definingCitationURI "http://www.scholarpedia.org/article/Conductance-based_models#Examples_and_Variants"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000091> a owl:Class ;
    rdfs:label "Wang-Buzsaki model"^^xsd:string ;
    skos:definition "a defined model that is a network model composed of single compartment models, derived from the original Hodgkin-Huxley model. This network model represents a network of hippocampal interneurons."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> ;
    OBOANN:definingCitationID "PMID: 8815919"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000092> a owl:Class ;
    rdfs:label "Fitzhugh-Nagumo model"^^xsd:string ;
    OBOANN:synonym "Bonhoeffer-van der Pol model"^^xsd:string ;
    skos:definition "a defined model that is a two-dimensional simplification of the Hodgkin-Huxley model of spike generation in squid giant axons. The model consist of a voltage-like variable having cubic nonlinearity that allows regenerative self-excitation via positive feedback and a recovery variable having a linear dynamics that provides a slower negative feedback."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> ;
    OBOANN:definingCitationID "PMID: 19431309"^^xsd:string ;
    OBOANN:definingCitationURI "http://www.scholarpedia.org/article/FitzHugh-Nagumo_model"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000093> a owl:Class ;
    rdfs:label "Morris-Lecar model"^^xsd:string ;
    skos:definition "a defined model that is a two-dimensional reduced excitation model applicable to systems having two non-inactivating voltage-sensitive conductances. The original form of the model employed an instantaneously responding voltage-sensitive calcium conductance for excitation and a delayed voltage-dependent potassium conductance for recovery."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000060> ;
    OBOANN:definingCitationID "PMID: 7260316"^^xsd:string ;
    OBOANN:definingCitationURI "http://www.scholarpedia.org/article/Morris-Lecar_model"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000094> a owl:Class ;
    rdfs:label "synaptic action"^^xsd:string ;
    skos:definition "a synapse model component that describes the impact of synaptic model on the post-synaptic model."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000037> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000095> a owl:Class ;
    rdfs:label "connection element"^^xsd:string ;
    skos:definition "a synapse model component that describe how the synapse is connected between two neuron models"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000037> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000096> a owl:Class ;
    rdfs:label "synaptic weight"^^xsd:string ;
    skos:definition "a synapse model component that refers to the strength or amplitude of a connection between two nodes, corresponding in biology to the amount of influence the firing of one neuron has on another."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000181> ;
    OBOANN:definingCitationURI "http://en.wikipedia.org/wiki/Synaptic_weight"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000097> a owl:Class ;
    rdfs:label "plasticity mechanism"^^xsd:string ;
    skos:definition "a synapse model component that relates to plasticity models."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000037> ;
    rdfs:comment ""^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000098> a owl:Class ;
    rdfs:label "synaptic conductance dynamic"^^xsd:string ;
    skos:definition "a synaptic model component that describes the dynamic of the synaptic conductance in the post-synaptic model"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000037> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000099> a owl:Class ;
    rdfs:label "synaptic delay"^^xsd:string ;
    skos:definition "delay between the activation of the synapse and its impact on the post-synaptic model."^^xsd:string ;
    rdfs:subClassOf SBO:0000225 .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000100> a owl:Class ;
    rdfs:label "voltage dependent receptor activation"^^xsd:string ;
    skos:definition "a synapse model component that describes the voltage dependent activation of  receptor such as NMDA receptor that will participate in the total synaptic current."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000037> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000101> a owl:Class ;
    rdfs:label "release mechanism"^^xsd:string ;
    skos:definition "a synapse model component that describes the neurotrasmitter release."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000037> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000102> a owl:Class ;
    rdfs:label "neurotransmittor diffusion"^^xsd:string ;
    skos:definition "a synapse model component that describes the diffusion of neurotransmitter in the extracellular space."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000037> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000103> a owl:Class ;
    rdfs:label "post-synaptic element"^^xsd:string ;
    skos:definition "a connection element which points to the model elements of the post-synaptic model that are used in the equations of the synapse model."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000095> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000104> a owl:Class ;
    rdfs:label "pre-synaptic element"^^xsd:string ;
    skos:definition "a connection element which points to the model elements of the pre-synaptic model that are used in the equations of the synapse model."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000095> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000105> a owl:Class ;
    rdfs:label "voltage-dependent current model"^^xsd:string ;
    skos:definition "an ionic current model that represents the current flowing through ionic channel triggered by the change in voltage."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000047> ;
    rdfs:comment ""^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000106> a owl:Class ;
    rdfs:label "voltage and ionic dependent current model"^^xsd:string ;
    skos:definition "an ionic current model that represents the current flowing through ionic channel triggered by the change of voltage and the binding of a particular ion."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000047> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000107> a owl:Class ;
    rdfs:label "supervised learning"^^xsd:string ;
    skos:definition "learning rule that produces an infered function to predict correct output value for any valid input object, based on an error function estimated through the training period with the presentation of input/output pairs."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000169> ;
    OBOANN:definingCitationURI "http://en.wikipedia.org/wiki/Supervised_learning"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000108> a owl:Class ;
    rdfs:label "ionic dependent current model"^^xsd:string ;
    skos:definition "a biochemically activated current model that represents the current flowing through ionic channel triggered by the binding of a particular ion."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000109> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000109> a owl:Class ;
    rdfs:label "biochemically activated current"^^xsd:string ;
    skos:definition "an ionic current model that represents the current flowing through ionic channels triggered by action of biochemical components."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000047> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000110> a owl:Class ;
    rdfs:label "unsupervised learning"^^xsd:string ;
    skos:definition "learning rule that produces an infered function based on unlabeled data without any feedback to evaluate the potential solution."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000169> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000111> a owl:Class ;
    rdfs:label "excitatory action"^^xsd:string ;
    skos:definition "a synaptic action that increases the post-synaptic model activity and/or voltage"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000094> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000112> a owl:Class ;
    rdfs:label "inhibitory action"^^xsd:string ;
    skos:definition "a synaptic action that decreases the post-synaptic activity and/or voltage"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000094> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000113> a owl:Class ;
    rdfs:label "shunting action"^^xsd:string ;
    skos:definition "a synaptic action that modifies the global conductance of the post-synaptic model"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000094> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000114> a owl:Class ;
    rdfs:label "instantaneous rise and monexponential decay"^^xsd:string ;
    skos:definition "a synaptic conductance dynamic which assumes an instanteanous rise of the conductance followed by an exponential decay with a time constant."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000098> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000115> a owl:Class ;
    rdfs:label "difference of two exponentials"^^xsd:string ;
    skos:definition "a synaptic conductance dynamic that is described as the difference of two exponential functions which represent the rising and the decay phase with two distinct time constants."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000098> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000116> a owl:Class ;
    rdfs:label "alpha function"^^xsd:string ;
    skos:definition "a synaptic conductance dynamic describing a conductance that has a rising phase with a certain rise time. As it has just a single time constant, the time course of the rise and decay are correlated."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000098> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000117> a owl:Class ;
    rdfs:label "cellular elements"^^xsd:string ;
    skos:definition "a network model component that refers to the cellular model type used to build the network model"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000058> ;
    rdfs:comment ""^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000118> a owl:Class ;
    rdfs:label "synaptic element"^^xsd:string ;
    skos:definition "a network model component that refers to the synapse model type used to build the network model"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000058> ;
    rdfs:comment ""^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000119> a owl:Class ;
    rdfs:label "connectivity pattern"^^xsd:string ;
    skos:definition "a model component quality that represents a connectivity pattern obtained with specific connectivity rule"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000202> ;
    rdfs:comment ""^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000120> a owl:Class ;
    rdfs:label "rate function"^^xsd:string ;
    skos:definition "a cellular model component that describes the dynamic of the cellular model firing rate."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000036> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000121> a owl:Class ;
    rdfs:label "recurrent"^^xsd:string ;
    skos:definition "a connectivity pattern in which connections between units form a directed cycle."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000119> ;
    OBOANN:definingCitationURI "http://en.wikipedia.org/wiki/Recurrent_neural_network#Fully_recurrent_network"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000122> a owl:Class ;
    rdfs:label "feedforward"^^xsd:string ;
    skos:definition "a connectivity pattern in which connections between units do not form a directed cycle."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000119> ;
    OBOANN:definingCitationURI "http://en.wikipedia.org/wiki/Feedforward_neural_networks"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000123> a owl:Class ;
    rdfs:label "small world"^^xsd:string ;
    skos:definition "a connectivity pattern in which most nodes are not neighbors of one another, but most node can be reached from every other by a small number of hops or steps."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000119> ;
    OBOANN:definingCitationURI "http://en.wikipedia.org/wiki/Small-world_network"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000124> a owl:Class ;
    rdfs:label "functional grouping"^^xsd:string ;
    skos:definition "cellular grouping quality that represents cellular grouping defined by common functional criterons."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000143> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000125> a owl:Class ;
    rdfs:label "anatomical grouping"^^xsd:string ;
    skos:definition "cellular grouping quality that represents cellular grouping based on anatomical information"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000143> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000126> a owl:Class ;
    rdfs:label "current model"^^xsd:string ;
    skos:definition "a cellular model component that represents the different types of modeled currents that can influence the trajectory of the membrane voltage"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000036> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000127> a owl:Class ;
    rdfs:label "current localization"^^xsd:string ;
    skos:definition "current model quality that represents the location of the current source on a neuronal model (somatic, primary dendrite, ...)"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000174> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000128> a owl:Class ;
    rdfs:label "current distribution"^^xsd:string ;
    skos:definition "current model quality that represents the spatial distribution of current sources (ionic channels, synaptic channels, stimulation electrode) on a compartment"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000174> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000129> a owl:Class ;
    rdfs:label "connectivity rule"^^xsd:string ;
    skos:definition "a network layout component that describe the rules used to establish connections between the cellular elements of the network model"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000177> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000130> a owl:Class ;
    rdfs:label "voltage"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000209> ;
    OBOANN:externalSourceURI "http://www.ebi.ac.uk/sbo/main/SBO:0000259"^^xsd:string .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000131> a owl:Class ;
    rdfs:label "capacitance"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000209> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000132> a owl:Class ;
    rdfs:label "current"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000181> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000133> a owl:Class ;
    rdfs:label "diameter"^^xsd:string ;
    rdfs:subClassOf SBO:0000465 .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000134> a owl:Class ;
    rdfs:label "length"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000209> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000135> a owl:Class ;
    rdfs:label "synaptic plasticity element"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000058> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000136> a owl:Class ;
    rdfs:label "frequency"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000181> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000137> a owl:Class ;
    rdfs:label "maximal conductance"^^xsd:string ;
    rdfs:subClassOf SBO:0000257 .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000138> a owl:Class ;
    rdfs:label "random"^^xsd:string ;
    skos:definition "cellular grouping quality that represents probabilistic grouping of cellular models within network models."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000143> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000139> a owl:Class ;
    rdfs:label "spike time"^^xsd:string ;
    rdfs:subClassOf SBO:0000346 .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000140> a owl:Class ;
    rdfs:label "time constant"^^xsd:string ;
    rdfs:subClassOf SBO:0000346 .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000141> a owl:Class ;
    rdfs:label "volume"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000209> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000142> a owl:Class ;
    rdfs:label "indices"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000181> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000143> a owl:Class ;
    rdfs:label "cellular grouping quality"^^xsd:string ;
    skos:definition "a model component quality that represent the properties of cellular grouping in network models"^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000202> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000144> a owl:Class ;
    rdfs:label "rate-based neuron model"^^xsd:string ;
    skos:definition "a cellular model that describe the firing activity of neuron using a rate function."^^xsd:string ;
    rdfs:subClassOf <http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000008> .

<http://purl.org/incf/ontology/Computational_Neurosciences/cno_alpha.owl#cno_0000145> a owl:Class ;
